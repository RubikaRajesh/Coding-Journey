//19. Remove Nth Node From End of List -- linkedlist - medium

class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        if(head==null|| head.next==null){
            return null;
        }
        ListNode first=head;
        int count=0;
        while(first!=null){
            count++;
            first=first.next;
        }
        if(count==n){
            return head.next;
        }
        first=head;
        int c=1;
        while(first!=null){
            if(c==count-n){
                first.next=first.next.next;
                break;
            }
            c++;
            first=first.next;
        }return head;
    }
}

//leetcode-122-medium
class Solution {
    public int maxProfit(int[] prices) {
        int max=0;
        int buy=prices[0];
        int profit=0;
        for(int i=1;i<prices.length;i++){
            if(buy>prices[i]){
                buy=prices[i];
            }
            else{
            profit=prices[i]-buy;
            max+=profit;
            buy=prices[i];
            }
        }return max;
    }
}

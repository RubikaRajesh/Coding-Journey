//206. Reverse Linked List- linked list - easy

class Solution {
    public ListNode reverseList(ListNode head) {
       if(head==null){
        return null;
       }
        if(head.next==null){
            return head;
        }
        ListNode prev=null;
        ListNode curr=head;
        while(curr!=null){
            ListNode next=curr.next;
            curr.next=prev;
            prev=curr;
            curr=next;
        }
        head=prev;
        return head;
       } 
}

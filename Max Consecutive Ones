//leetcode-arrays-easy-Max Consecutive Ones
class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int curr=0;
        int max=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]==1){
                curr++;          
            }
            if(nums[i]==0){
                max=Math.max(curr,max);
                curr=0;
            }
        }return Math.max(curr,max);
    }
}
